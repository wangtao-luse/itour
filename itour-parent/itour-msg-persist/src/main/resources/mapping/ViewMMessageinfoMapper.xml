<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itour.persist.ViewMMessageinfoMapper">

    <!-- 开启二级缓存 -->

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.itour.model.msg.ViewMMessageinfo">
        <result column="ID" property="id" />
        <result column="SUBJECT" property="subject" />
        <result column="TEXT" property="text" />
        <result column="FROM" property="from" />
        <result column="TO" property="to" />
        <result column="TYPE" property="type" />
        <result column="SENDTIME" property="sendtime" />
        <result column="AIM" property="aim" />
        <result column="ORIGIN" property="origin" />
        <result column="IP" property="ip" />
        <result column="AIM_STR" property="aimStr" />
        <result column="ORIGIN_STR" property="originStr" />
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, SUBJECT, TEXT, FROM, TO, TYPE, SENDTIME, AIM, ORIGIN, IP, AIM_STR, ORIGIN_STR
    </sql>

</mapper>
