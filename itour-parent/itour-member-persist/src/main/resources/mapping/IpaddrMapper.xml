<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itour.persist.IpaddrMapper">

    <!-- 开启二级缓存 -->

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.itour.model.member.Ipaddr">
        <id column="ID" property="id" />
        <result column="IP" property="ip" />
        <result column="ISP" property="isp" />
        <result column="COUNTRY" property="country" />
        <result column="COUNTRYCODE" property="countrycode" />
        <result column="REGION" property="region" />
        <result column="REGIONNAME" property="regionname" />
        <result column="CITY" property="city" />
        <result column="AREA" property="area" />
        <result column="LATITUDE" property="latitude" />
        <result column="LONGITUDE" property="longitude" />
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, IP, ISP, COUNTRY, COUNTRYCODE, REGION, REGIONNAME, CITY, AREA, LATITUDE, LONGITUDE
    </sql>

</mapper>
