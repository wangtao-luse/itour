<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itour.persist.WebsiteVisitInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.itour.model.dictionary.WebsiteVisitInfo">
        <id column="ID" property="id" />
        <result column="PV" property="pv" />
        <result column="VV" property="vv" />
        <result column="IP" property="ip" />
        <result column="UV" property="uv" />
        <result column="COUNT_DATE" property="countDate" />
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, PV, VV, IP, UV, COUNT_DATE
    </sql>
<select id="selectRecentlyOne" resultType="com.itour.model.dictionary.WebsiteVisitInfo">
select * FROM t_w_work_info c where c.id= (SELECT max(id)FROM t_w_work_info d);
</select>
</mapper>
